{
  "name": "fastify",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "node dist/app.js",
    "start:pm2": "pm2 start ecosystem.config.js",
    "start:pm2:prod": "pm2 start ecosystem.config.js --env production",
    "stop:pm2": "pm2 stop committee-api",
    "restart:pm2": "pm2 restart committee-api",
    "delete:pm2": "pm2 delete committee-api",
    "logs:pm2": "pm2 logs committee-api",
    "monit:pm2": "pm2 monit",
    "build": "tsc",
    "type-check": "tsc --noEmit",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "dev": "tsnd --respawn --watch --transpile-only --exit-child src/app.ts",
    "test": "vitest",
    "test:run": "vitest run",
    "test:ui": "vitest --ui",
    "prepare": "node -e \"try { if (!process.env.CI && require('fs').existsSync('.git')) { require('child_process').execSync('husky', { stdio: 'inherit' }); } } catch (e) { process.exit(0); }\""
  },
  "dependencies": {
    "@fastify/autoload": "^5.10.0",
    "@fastify/cors": "^9.0.1",
    "@fastify/helmet": "^11.1.1",
    "@fastify/jwt": "^8.0.1",
    "@fastify/multipart": "^8.3.0",
    "@fastify/rate-limit": "^7.6.0",
    "@fastify/request-context": "^5.1.0",
    "@fastify/swagger": "^8.14.0",
    "@fastify/swagger-ui": "^4.0.0",
    "@prisma/client": "^5.16.1",
    "@types/uuid": "^10.0.0",
    "dotenv": "^16.4.5",
    "fastify": "^4.28.0",
    "file-type": "^20.5.0",
    "jsonwebtoken": "^9.0.2",
    "kill-port": "^2.0.1",
    "mobile-detect": "^1.4.5",
    "pg": "^8.13.1",
    "redis": "^4.6.15",
    "uuid": "^13.0.0",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0",
    "zod": "^3.23.1",
    "zod-to-json-schema": "^3.23.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^22.19.1",
    "@types/pg": "^8.11.10",
    "@typescript-eslint/eslint-plugin": "^8.49.0",
    "@typescript-eslint/parser": "^8.49.0",
    "@vitest/ui": "^4.0.14",
    "eslint": "^9.39.1",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-node": "^11.1.0",
    "globals": "^16.5.0",
    "husky": "^9.1.7",
    "prisma": "^5.16.1",
    "ts-node-dev": "^2.0.0",
    "vitest": "^4.0.14"
  }
}
